{
  "name": "Score247-Web-App",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:package": "npm install",
    "dev": "ts-node --project tsconfig.server.json server.ts",
    "build:test": "cross-env SETTINGENV=test next build && tsc --project tsconfig.server.json",
    "build:production": "cross-env SETTINGENV=production next build && tsc --project tsconfig.server.json",
    "start:test": "cross-env SETTINGENV=production NODE_ENV=production PORT=8400 node dist/server.js",
    "start": "cross-env SETTINGENV=production NODE_ENV=production PORT=8500 node dist/server.js",
    "start:dev": "cross-env SETTINGENV=production NODE_ENV=production PORT=8600 node dist/server.js",
    "start:autotest": "cross-env SETTINGENV=production NODE_ENV=production PORT=8700 node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "export": "next export",
    "lighthouse:run:desktop": "node node_modules/lighthouse/lighthouse-cli/index.js --config-path _build_scripts/audit-desktop-config.js --output json --output html --output-path ./html_desktop_audit.json --chrome-flags='--headless' https://score247-web-test.nexdev.net/",
    "lighthouse:run:mobile": "node node_modules/lighthouse/lighthouse-cli/index.js --config-path _build_scripts/audit-mobile-config.js --output json --output html --output-path ./html_mobile_audit.json --chrome-flags='--headless' https://score247-web-test.nexdev.net/",
    "lighthouse:report": "node ./audit.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
